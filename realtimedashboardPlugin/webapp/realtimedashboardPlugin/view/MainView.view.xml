    <Panel id="_IDGenPanel">
        <HBox
            id="tileContainer"
            class="sapUiTinyMarginBegin sapUiTinyMarginTop" items="{data>/}">
            <!-- <FlexBox
                id="_IDGenFlexBox1M"
                justifyContent="Center"
                class="sapUiSmallMargin"
            > -->
                <f:Card
                    id="_IDGenCard1"
                    class="sapUiMediumMargin"
                    width="220px"
                    
            
                >
                    <!-- <f:customData>
                        <core:CustomData key="style-class"
                            value="{data>status}"
                            writeToDom="true"/>
                    </f:customData> -->
                    <f:header>
                        <card:Header
                            id="_IDGenHeader"
                            press=".onCardTitlePress"
                        >
                            <card:customData>
                                <core:CustomData key="style-class"
                                    value="{data>status}"
                                    writeToDom="true"/>
                            </card:customData>
                        </card:Header>
                    </f:header>

                    <f:content>
                        <VBox
                            id="_IDGenVBox"
                            class="sapUiSmallMargin"
                            height="auto"
                        >
                            <customData>
                                <core:CustomData
                                    key="status"
                                    value="{data>status}"
                                    writeToDom="true"
                                />
                            </customData>

                            <HBox
                                id="_IDGenHBox"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText1"
                                    text="{i18n>card-resourceLab}:"
                                />

                                <Text
                                    id="_IDGenText2"
                                    text="{data>resource}"
                                />
                            </HBox>

                            <HBox
                                id="_IDGenHBox1"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText3"
                                    text="{i18n>card-operatorLab}:"
                                />

                                <Text
                                    id="_IDGenText4"
                                    text="{data>plant}"
                                />
                            </HBox>

                            <HBox
                                id="_IDGenHBox2"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText5"
                                    text="{i18n>card-orderLab}:"
                                />

                                <Text
                                    id="_IDGenText6"
                                    text="{data>description}"
                                />
                            </HBox>

                            <HBox
                                id="_IDGenHBox3"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText7"
                                    text="{i18n>card-Component}:"
                                />

                                <Text
                                    id="_IDGenText8"
                                    text="{data>status}"
                                />
                            </HBox>

                            <HBox
                                id="_IDGenHBox4"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText9"
                                    text="{i18n>card-target}:"
                                />

                                <Text
                                    id="_IDGenText10"
                                    text="{data>setupState}"
                                />
                            </HBox>

                            <HBox
                                id="_IDGenHBox5"
                                alignItems="Center"
                                justifyContent="SpaceBetween"
                                width="100%"
                            >
                                <Text
                                    id="_IDGenText11"
                                    text="{i18n>card-Material}:"
                                />

                                <Text
                                    id="_IDGenText12"
                                    text="{data>plant}"
                                />
                            </HBox>
                        </VBox>
                    </f:content>
                </f:Card>
            <!-- </FlexBox> -->
        </HBox>
    </Panel>

    <!-- </content>
	    </Panel>		 -->
</mvc:View>
